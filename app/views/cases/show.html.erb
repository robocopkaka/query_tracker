<h1>Case <%= @case.id%></h1>

<p> Description: <%= @case.description %></p>
<p>Created by: <%= @case.user.name %></p>
<%= form_tag("/assign", remote: true) do %>
	<%= select_tag(:support_id, options_for_select(@users), class: 'form-control') %>
	<%= submit_tag "Assign", class: 'btn btn-primary' %>
<% end %>